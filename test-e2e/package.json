{
  "name": "blockstack-sernityjs-test-e2e",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf target",
    "serenity:update": "serenity update",
    "webdriver:update": "webdriver-manager update --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3)",
    "bs-start": "node src/browserstack-local/start-browserstack-local.js",
    "bs-stop": "node src/browserstack-local/stop-browserstack-local.js",
    "protractor": "protractor protractor.conf.js",
    "pretest-e2e": "serenity update",
    "report": "serenity run",
    "test-e2e": "failsafe clean serenity:update webdriver:update protractor report",
    "test-e2e:browserstack": "failsafe clean serenity:update bs-start protractor report bs-stop"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
