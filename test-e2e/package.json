    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^8.10.49",
    "browserstack-local": "^1.4.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^1.3.3",
    "cypress-serenity-reporter": "^1.0.1",
    "npm-failsafe": "^0.4.1",
    "open": "^6.4.0",
    "opn-cli": "^3.1.0",
    "protractor": "^5.4.2",
    "rimraf": "^2.6.2",
    "serenity": "^1.1.6",
    "serenity-cli": "^0.11.3",
    "serenity-js": "^1.10.13",
